# Sui Network Configuration (required for wallet balance, deposits, AI Insight, and Blitz)
NEXT_PUBLIC_SUI_NETWORK=testnet

# Sui RPC Endpoint
NEXT_PUBLIC_SUI_RPC_ENDPOINT=https://fullnode.testnet.sui.io:443

# Treasury Contract Configuration (replace with your deployed contract IDs)
NEXT_PUBLIC_TREASURY_PACKAGE_ID=0x...
NEXT_PUBLIC_TREASURY_OBJECT_ID=0x...

# USDC Token Type (must match the network above)
# Sui Testnet native USDC:
NEXT_PUBLIC_USDC_TYPE=0xa1ec7fc00a6f40db9693ad1415d0c193ad3906494428cf252621037bd7117e29::usdc::USDC
# For mainnet, use Circle's native USDC type on Sui (see https://developers.circle.com/stablecoins/quickstart-setup-transfer-usdc-sui)

# Application Configuration
NEXT_PUBLIC_APP_NAME=Overflow
NEXT_PUBLIC_ROUND_DURATION=30
NEXT_PUBLIC_PRICE_UPDATE_INTERVAL=1000
NEXT_PUBLIC_CHART_TIME_WINDOW=300000

# Supabase Configuration (replace with your Supabase project credentials)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
